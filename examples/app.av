module App
    depends [Examples.Fibonacci]
    intent:
        "Demonstrates module imports via depends."

fn main() -> Unit
    ! [Console]
    val result = Examples.Fibonacci.buildFibStats(10)
    match result:
        Result.Ok(stats) -> Console.print(stats.sum)
        Result.Err(msg)  -> Console.print(msg)

verify imports:
    Examples.Fibonacci.fib(10) => 55
    Examples.Fibonacci.isEven(144) => true
